{
  "expo": {
    "name": "air-command-app",
    "slug": "u-control",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.air-command",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Disclaimer de uso de dados do usu√°rio"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/logo/icon.png"
        }
      ],
      "expo-tracking-transparency"
    ],
    "extra": {
      "eas": {
        "projectId": "7725650d-aed3-4dfa-8e18-f9e1d8383edf"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "mscamargo"
  }
}
